{
  "name": "fellowship",
  "version": "1.0.0",
  "description": "Bitwise based ACL",
  "main": "index.js",
  "publishConfig": {
    "registry": "http://registry.npmjs.org"
  },
  "scripts": {
    "lint": "eslint -f unix .",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "pretest": "npm run lint",
    "test": "istanbul cover node_modules/mocha/bin/_mocha  -- --recursive",
    "posttest": "istanbul check-coverage --statements 90 --branches 75 --functions 100 --lines 95 || echo open coverage/lcov-report/index.html to find out more",
    "prepublish": "npm test && npm prune"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:confuser/node-fellowship.git"
  },
  "keywords": [
    "acl",
    "bitwise",
    "group",
    "groups",
    "perms",
    "permissions"
  ],
  "author": "James Mortemore <jamesmortemore@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/confuser/node-fellowship/issues"
  },
  "dependencies": {
    "async.each": "^0.5.2"
  },
  "devDependencies": {
    "@confuser/eslint-config": "^2.0.0",
    "coveralls": "^2.11.2",
    "eslint": "^2.10.2",
    "eslint-plugin-standard": "^1.3.2",
    "istanbul": "^0.4.1",
    "mocha": "^2.3.2"
  }
}
